-- 1. [LEFT JOIN]
-- Question: Which VIPs are presenting at events and what type of event is it?

SELECT 
    vips.first_name,
    vips.last_name,
    events.event_name,
    events.event_type
FROM vips
LEFT JOIN events
    ON vips.event = events.event_id;


-- 2. [INNER JOIN]
-- Question: Which VIPs have hotel reservations?

SELECT 
    vips.first_name,
    vips.last_name,
    reservations.hotel
FROM vips
INNER JOIN reservations
    ON vips.member_id = reservations.member_id;


-- 3. [MULTIPLE JOINS]
-- Question: Which VIPs are staying at hotels and what are the hotel names?

SELECT 
    vips.first_name,
    vips.last_name,
    hotels.hotel_name,
    hotels.address
FROM vips
JOIN reservations
    ON vips.member_id = reservations.member_id
JOIN hotels
    ON reservations.hotel = hotels.hotel_id;


-- 4. [SELF JOIN]
-- Question: Who gives feedback to whom?

SELECT 
    a.first_name AS reviewer_first_name,
    a.last_name AS reviewer_last_name,
    b.first_name AS reviewed_first_name,
    b.last_name AS reviewed_last_name
FROM vips a
JOIN vips b
    ON a.provides_feedback_to = b.member_id;


-- 5. [YOUR CHOICE – BIT DATA TYPE]
-- Question: Who RSVPed YES to BOTH welcome party and dinner?

SELECT 
    vips.first_name,
    vips.last_name
FROM vips
JOIN reservations
    ON vips.member_id = reservations.member_id
WHERE welcome_rsvp = 1
  AND dinner_rsvp = 1;


-- 6. [SUBQUERY]
-- Question: Which VIPs are presenting at keynote events?

SELECT first_name, last_name
FROM vips
WHERE event IN (
    SELECT event_id 
    FROM events 
    WHERE event_type = 'Keynote'
);
